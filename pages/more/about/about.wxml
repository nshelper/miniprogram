<view class="about">
  <view class="app">
    <image class="app__logo" src="/images/logo.png"></image>
    <view class="app__info">
      <text class="app__name">内师助手</text>
      <text class="app__version">{{version}}</text>
    </view>
    
     <view class="waveWrapper waveAnimation">
         <view class="waveWrapperInner bgTop">
            <view class="wave waveTop" style="background-image:url('http://image.weilanwl.com/img/wave-top.png')"></view>
        </view> 
          <view class="waveWrapperInner bgMiddle">
            <view class="wave waveMiddle" style="background-image:url('http://image.weilanwl.com/img/wave-mid.png')"></view>
        </view>
         <view class="waveWrapperInner bgBottom">
            <view class="wave waveBottom" style="background-image:url('http://image.weilanwl.com/img/wave-bot.png')"></view>
        </view>   
    </view>

  </view>
  <view class="menu-list">
    <navigator url="opensource" class="menu-list__item">
      <text>开源协议</text>
      <image class="menu-list__r-icon" src="/images/more/arrow_right.png"></image>
    </navigator>
    <navigator url="donate" class="menu-list__item">
      <text>捐赠致谢</text>
      <image class="menu-list__r-icon" src="/images/more/arrow_right.png"></image>
    </navigator>
    <button open-type="feedback" class="menu-list__item menu-list__contact">
      <text>意见反馈</text>
      <image class="menu-list__r-icon" src="/images/more/arrow_right.png"></image>
    </button>
    <button open-type="contact" class="menu-list__item menu-list__contact">
      <text>联系客服</text>
      <image class="menu-list__r-icon" src="/images/more/arrow_right.png"></image>
    </button>
  </view>

  <!-- 版权 -->
  <view class="copyright">
    <text class="qq-group">wechat:strongdreams</text>
    <text>Copyright (c) 2018 STRUGGLER</text>
  </view>
</view>
